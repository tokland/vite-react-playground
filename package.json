{
    "name": "d2-skeleton",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "build": "tsc && vite build --outDir=build",
        "build-dependencies-visualizer": "vite-bundle-visualizer",
        "code-quality": "yarn lint && yarn test",
        "generate-docs": "typedoc --entryPointStrategy expand ./src",
        "lint": "eslint src",
        "lint-unused": "ts-prune --error",
        "run-script": "ts-node",
        "start": "vite",
        "test": "yarn test-unit && yarn test-integration",
        "test-unit": "vitest run",
        "test-unit-watch": "vitest watch",
        "test-integration": "playwright test --reporter=list",
        "test-integration-watch": "PWTEST_WATCH=1 playwright test --reporter=list"
    },
    "dependencies": {
        "@rimbu/hashed": "^0.9.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "real-cancellable-promise": "^1.1.2",
        "type-route": "^1.0.1",
        "zustand": "^4.3.7"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-0",
        "@babel/plugin-syntax-flow": "^7.14.5",
        "@babel/plugin-transform-react-jsx": "^7.14.9",
        "@playwright/test": "^1.33.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@types/node": "^18.15.11",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@vitejs/plugin-react": "^3.1.0",
        "cmd-ts": "^0.12.1",
        "eslint": "^8.37.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "expect-type": "^0.15.0",
        "jsdom": "^21.1.1",
        "prettier": "^2.8.7",
        "ts-node": "^10.9.1",
        "ts-prune": "^0.10.3",
        "typedoc": "^0.24.6",
        "typescript": "^4.9.3",
        "vite": "^4.2.0",
        "vite-bundle-visualizer": "^0.6.0",
        "vite-plugin-eslint": "^1.8.1",
        "vitest": "^0.29.8"
    }
}
